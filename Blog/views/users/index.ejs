<%- include('../partials/header')%>
<figure>
  <img src="<%= user.avatar%>" />
</figure>
<figcaption>
  <h4><%= user.name%></h4>
</figcaption>
<table>
  <thead>
    <tr>
      <th>Blogs</th>
      <th>Comments</th>
    </tr>
  </thead>
  <tbody>
    <%blogs.forEach(function(b){ if(b.author === user._id){%>
    <tr>
      <td><%= b.title%></td>
      <%}%>
    </tr>
    <%})%>
  </tbody>
</table>
<%- include('../partials/footer')%>
